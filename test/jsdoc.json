{
    "plugins": [
        "plugins/markdown",
        "plugins/underscore"
    ],
    "markdown": {
        "idInHeadings": true
    },
    "source": {
        "include": [
            "./", "package.json"
        ],
        "exclude": [
            "./node_modules",
            "./docs/"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)(_|.+\\.test\\.+)"
    },
    "typescript": {
        "moduleRoot": "./"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "template": "../",
        "encoding": "utf8"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "metadata": {
        "title": "AppKu: JSDoc Template (Test)",
        "logo": "https://static.appku.com/logo/color/png/appku-ninja-slogan-horizontal-256x86.png",
        "links": [
            { "icon": "github", "url": "https://github.com/appku/jsdoc-template" },
            { "icon": "twitter", "url": "https://twitter.com" }
        ]
    }
}